<section class="sn-banner">
	<div id="myCarousel" class="carousel slide">
   <ol class="carousel-indicators">
      <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
      <li data-target="#myCarousel" data-slide-to="1"></li>
      <li data-target="#myCarousel" data-slide-to="2"></li>

   </ol>
   <div class="carousel-inner">
      <div class="item active">
         <img src="image/banner1.png" class="hidden-xs" alt="First slide">
      </div>
      <div class="item">
         <img src="image/banner3.png" alt="Third slide">
      </div>
			<div class="item">
         <img src="image/banner4.png" alt="Third slide">
      </div>
   </div>
   <a class="carousel-control" href="#myCarousel"
      data-slide="prev">&lsaquo;</a>
   <a class="carousel-control ri" href="#myCarousel"
      data-slide="next">&rsaquo;</a>
</div>
</section>
<script>
	$(function(){
		function pageEvent(){//屏蔽滚轮 侦听左右
      document.onkeydown=function(event){
        var e = event || window.event || arguments.callee.caller.arguments[0];
        if(e && e.keyCode==37){//上,左
          $("#myCarousel").carousel('prev');;
        }
        if(e && e.keyCode==39){//下,右
          $("#myCarousel").carousel('next');
        }
      }
    }
    pageEvent();
	})
</script>
